<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Student Roll Numbers -->
        <record id="seq_school_student_roll" model="ir.sequence">
            <field name="name">Student Roll Number</field>
            <field name="code">school.student.roll</field>
            <field name="prefix">ROLL/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Student Admission Numbers -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">Student Admission Number</field>
            <field name="code">school.student</field>
            <field name="prefix">ADM/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 